
#include<stdio.h>

int main()
{
	int a[10][10],m,n,i,j,b[10][10];
	int sum=0;
	printf("Enter the values of m and n:");
	scanf("%d %d",&m,&n);
	printf("Enter the matrix elements");
	for(i=0;i<n;i++)
	{
	    for(j=0;j<m;j++)
	    {
	        scanf("%d",&a[i][j]);
	    }
	}

	for(i=0;i<n;i++)
	{
	    for(j=0;j<m;j++)
	    {
	        if(i==j)
	        {
	            sum=sum+a[i][j];
	        }
	    }
	}
	printf("The sum of diagobal element is:%d",sum);

	
 return 0;
}
